{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "projects": {
    "unshuffle": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "unshuffle/apps/ui/static/angular/",
            "index": "unshuffle/apps/ui/angular/index.html",
            "main": "unshuffle/apps/ui/angular/main.ts",
            "polyfills": "unshuffle/apps/ui/angular/polyfills.ts",
            "tsConfig": "unshuffle/apps/ui/angular/tsconfig.json",
            "aot": true,
            "scripts": []
          }
        }
      }
    }
  },
  "defaultProject": "unshuffle"
}
